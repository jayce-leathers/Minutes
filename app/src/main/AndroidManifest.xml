<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="apps.jayceleathers.me.minutes">

    <application
        tools:replace="android:icon"
        android:allowBackup="true"
        android:icon="@drawable/logo1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.orm.SugarApp">
        <meta-data android:name="VERSION" android:value="2" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="apps.jayceleathers.me.data" />
        <activity
            android:name="apps.jayceleathers.me.activities.MainActivity"
            android:logo="@drawable/logo1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
